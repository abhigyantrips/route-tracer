{
  "name": "route-tracer",
  "displayName": "Route Tracer",
  "version": "0.0.1",
  "description": "A browser extension that traces the path of a network packet over the Internet.",
  "author": "Abhigyan Trips <contact@abhigyantrips.dev>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.89.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs"
    ]
  },
  "pnpm": {
    "overrides": {
      "msgpackr@<1.10.1": ">=1.10.1",
      "svelte@<4.2.19": ">=4.2.19"
    }
  }
}